{
  "name": "hedgeai-platform",
  "version": "1.0.0",
  "description": "HedgeAI Derivative Hedging Platform with RL",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\" --names \"BACKEND,FRONTEND\" --prefix-colors \"blue,magenta\" --kill-others",
    "backend": "cd backend && npm run dev",
    "frontend": "cd frontend && npm run dev",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "dependencies": {
    "@hugeicons/core-free-icons": "^3.1.1",
    "@hugeicons/react": "^1.1.5",
    "clsx": "^2.1.1",
    "framer-motion": "^12.34.3",
    "lucide-react": "^0.575.0",
    "tailwind-merge": "^3.5.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
